# To create a function to normalise and standardise numeric variables
standardise <- function(X) {
cols <- ncol(X)
for (c in 1:cols) {
if (is.numeric(X[, c])) {
start <- ncol(X)
c.c <- (X[, c] - mean(X[, c], na.rm=TRUE)) / (2 * sd(X[, c], na.rm=TRUE))
X[start+1] <- c.c
colnames(X)[start+1] <- paste0("c.", colnames(X)[c])
}
}
return(X)
}
